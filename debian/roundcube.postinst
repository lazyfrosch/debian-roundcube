#!/bin/sh

set -e

# Handle old files that are not needed any more
for OLD in /etc/logrotate.d/roundcube /etc/default/roundcube /etc/cron.daily/roundcube; do
    [ -f $OLD ] && {
	if [ ! -s $OLD ]; then
	    rm $OLD
	else
	    grep -q '^# 75321 DELETE 75321$' $OLD && rm $OLD
	fi
    }
done

#DEBHELPER#

exit 0
